{
  "name": "webumer/bcb-php-sdk",
  "description": "Unofficial PHP SDK for BCB Group Client API (payments, beneficiaries, virtual IBANs, webhooks).",
  "version": "1.2.3",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "bcb",
    "bcb-group",
    "api",
    "sdk",
    "php",
    "payments",
    "virtual-iban",
    "webhooks"
  ],
  "homepage": "https://github.com/webumer/bcb-php-sdk",
  "support": {
    "issues": "https://github.com/webumer/bcb-php-sdk/issues",
    "source": "https://github.com/webumer/bcb-php-sdk"
  },
  "authors": [
    {
      "name": "Umer Sulman"
    }
  ],
  "autoload": {
    "psr-4": {
      "Webumer\\Bcb\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8",
    "psr/http-client": "^1.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}